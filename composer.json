{
    "name": "sunnysideup/ecommerce_test",
    "type": "silverstripe-module",
    "description": "allows you to review sunnysideup/ecommerce",
    "authors": [
        {
            "name": "Nicolaas Francken",
            "email": "modules@sunnysideup.co.nz",
            "role": "lead developer"
        }
    ],
    "require": {
        "silverstripe/geoip": "*",
        "sunnysideup/ecommerce": "*",
        "silverstripe/recipe-cms": "^4.5.2",
        "php": ">=5.6"
    },
    "scripts": {
        "post-install-cmd": [
            "php framework/cli-script.php dev/build flush=all"
        ],
        "post-update-cmd": [
            "php framework/cli-script.php dev/build flush=all"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "Silverstripe-CMS",
        "CMS",
        "Silverstripe",
        "ecommerce",
        "e-commerce",
        "shop",
        "sales",
        "demo"
    ],
    "extra": {
        "installer-name": "ecommerce_test",
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ]
    },
    "license": "BSD-3-Clause",
    "require-dev": {
        "sunnysideup/huringa": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Sunnysideup\\EcommerceTest\\": "app/src/"
        }
    }
}