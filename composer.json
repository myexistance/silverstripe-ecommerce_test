{
    "name": "sunnysideup/ecommerce_test",
    "type": "silverstripe-module",
    "description": "allows you to review sunnysideup/ecommerce",
    "authors": [
        {
            "name": "Nicolaas Francken",
            "email": "modules@sunnysideup.co.nz",
            "role": "lead developer"
        }
    ],
    "require": {
        "silverstripe/framework": "3.6.1",
        "silverstripe/cms": "3.6.1",
        "silverstripe/geoip": "*",

        "sunnysideup/ecommerce": "dev-master",
        "sunnysideup/ecommerce_tax": "dev-master",
        "sunnysideup/ecommerce_delivery": "dev-master",
        "sunnysideup/ecommerce_product_variation": "dev-master",
        "sunnysideup/payment_paypal": "dev-master",
        "sunnysideup/ecommerce_dashboard": "dev-master",
        "sunnysideup/ecommerce_orderstep_feedback": "dev-master",
        "sunnysideup/cms_edit_link_field": "dev-master",
        "sunnysideup/permission_provider": "dev-master",
        "sunnysideup/google_address_field": "dev-master",

        "sunnysideup/dataintegritytests": "dev-master",
        "sunnysideup/googleanalyticsbasics": "dev-master",
        "sunnysideup/metatags": "dev-master",
        "sunnysideup/typography": "dev-master",
        "sunnysideup/templateoverview": "dev-master",

        "symbiote/silverstripe-grouped-cms-menu": "*",
        "symbiote/silverstripe-gridfieldextensions": "*",

        "unclecheese/betterbuttons": "*",
        "unclecheese/dashboard": "*",

        "sunnysideup/themes_ssu_flava_main": "dev-master",
        "sunnysideup/themes_ssu_flava_main_cms": "dev-master",
        "sunnysideup/themes_ssu_flava_main_ecommerce": "dev-master",
        "sunnysideup/themes_ssu_flava_main_ecommerce_product_variation": "dev-master",
        "sunnysideup/themes_ssu_flava_main_webportfolio": "dev-master",
        "sunnysideup/themes_ssu_flava_simple": "dev-master",
        "sunnysideup/themes_ssu_flava_simple_ecommerce": "dev-master",
        "sunnysideup/ecommerce_bootstrap_theme": "dev-master",

        "sunnysideup/webpack_requirements_backend": "dev-master",
        "sunnysideup/sswebpack_engine_only": "dev-master",
        "sunnysideup/skeleton": "dev-master",
        "sunnysideup/sskube": "dev-master",

        "gdmedia/ss-auto-git-ignore": "dev-master"
    },
    "scripts": {
        "post-install-cmd": [
            "sed -i '/CLEANING_MODE_ALL/d' ./framework/model/Aggregate.php",
            "php framework/cli-script.php dev/build flush=all"
        ],
        "post-update-cmd": [
            "GDM\\SSAutoGitIgnore\\UpdateScript::Go",
            "sed -i '/CLEANING_MODE_ALL/d' ./framework/model/Aggregate.php",
            "php framework/cli-script.php dev/build flush=all"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "Silverstripe-CMS",
        "CMS",
        "Silverstripe",
        "ecommerce",
        "e-commerce",
        "shop",
        "sales",
        "demo"
    ],
    "extra": {
        "installer-name": "ecommerce_test"
    }
}
