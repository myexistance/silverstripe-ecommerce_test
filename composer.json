    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": []
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "Silverstripe-CMS",
        "CMS",
        "Silverstripe",
        "ecommerce",
        "e-commerce",
        "shop",
        "sales",
        "demo"
    ],
    "extra": {
        "installer-name": "ecommerce_test",
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ]
    },
    "license": "BSD-3-Clause",
    "autoload": {
        "psr-4": {
            "Sunnysideup\\EcommerceTest\\": "app/src/"
        },
        "files": [
            "app/src/Page.php",
            "app/src/PageController.php"
        ]
    }
}
