{
    "name": "sunnysideup/ecommerce_test",
    "type": "silverstripe-module",
    "description": "allows you to review sunnysideup/ecommerce",
    "authors": [
        {
            "name": "Nicolaas Francken",
            "email": "modules@sunnysideup.co.nz",
            "role": "lead developer"
        }
    ],
    "require": {
        "silverstripe/framework": "~3.6",
        "silverstripe/cms": "~3.6",
        "silverstripe/geoip": "*",
        "sunnysideup/ecommerce": "*",
    },
    "scripts": {
        "post-install-cmd": [
            "php framework/cli-script.php dev/build flush=all"
        ],
        "post-update-cmd": [
            "php framework/cli-script.php dev/build flush=all"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "Silverstripe-CMS",
        "CMS",
        "Silverstripe",
        "ecommerce",
        "e-commerce",
        "shop",
        "sales",
        "demo"
    ],
    "extra": {
        "installer-name": "ecommerce_test"
    },
    "license": "BSD-3-Clause"
}
